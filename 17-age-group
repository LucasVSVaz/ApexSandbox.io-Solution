public String ageGroup(Integer n) {
    //code here

    Boolean infant = (n >= 0 && n <= 1) ? true : false;

    if(infant){
        return 'Infant';
    }
    else if (n >= 2 && n <= 14){
        return 'Child';
    }
    else if (n >= 15 && n <= 21){
        return 'Youth';
    }
    return 'Adult';
}