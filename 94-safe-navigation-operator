public String getAccountBillingCityWithSafeNavigation(List<Account> accounts){
   // Code here
   String getCity = '';
   List<String> getBillingCity = new List<String>();

   for(Account acct : accounts){
       getBillingCity.add(acct?.BillingCity);
   }
   

   return getBillingCity[0].toUpperCase();
}
