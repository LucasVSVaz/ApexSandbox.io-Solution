public Id insertStudent(String name, String email) {
    // code here

    apxio__Student__c newStudent = apxio__Student__c(
        Name = name;
        apxio__Email__c = email;
    );

    List<apxio__Student__c> addStudent = new apxio__Student__c(){newStudent};

    insert addStudent;

    return addStudent[0].id;
    
}