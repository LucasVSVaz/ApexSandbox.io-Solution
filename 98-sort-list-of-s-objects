public List<Account> getAccounts(List<Account> accounts) {
    // code here

    if(accounts == null){
        return null;
    }

    List<AccountWrapper> acctWrapper = new List<AccountWrapper>();

    for(Account acct : accounts){
        acctWrapper.add(new AccountWrapper(acct));
    }

    acctWrapper.sort();

    for(Integer i = 0; i < accounts.size(); i++) {
        accounts[i] = acctWrapper[i].acc;
    }

    return accounts;

}


// write extra class here 
public class AccountWrapper implements Comparable{

    public Account acct;

    public AccountWrapper (Account actt){
        this.acct = acct;
    }

    public Integer compareTo (Object compareTo){
        AccountWrapper compareToAcct = (AccountWrapper)compareTo;
        if (compareToAcct.acct.annualRevenue == this.acct.annualRevenue){
            return 0;
        } 
        if(compareToAcct.acct.annualRevenue > this.acct.annualRevenue){
            return 1;
        }
        return -1;
    }
}
