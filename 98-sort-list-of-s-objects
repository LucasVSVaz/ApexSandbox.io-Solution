public List<Account> getAccounts(List<Account> accounts) {
    // code here

    List<Account> sortedList = new List<Account>{accounts.sort()}

    List<Account> accountsResult = new List<Account>();

    for(Integer i = sortedList.size() - 1; i >= 0; i--){
        accountsResult.add(accounts[i]);
    }

    return accountsResult;
}
