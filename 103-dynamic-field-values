public List<String> getFieldsValue(Account acc, List<String> fields)
{
    // code here
    fields.add(String.valueOf(acc?.get('Name')));
    fields.add(String.valueOf(acc?.get('BillingCity')));
    fields.add(String.valueOf(acc?.get('AnnualRevenue')));
    fields.add(String.valueOf(acc?.get('Rating')));
    
    return fields;
}