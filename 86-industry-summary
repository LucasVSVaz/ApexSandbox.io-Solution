public Map<String, Decimal> industrySummary(List<Account> accounts) {
    //code here

Map<String, Decimal> getMap = new Map<String, Decimal>();


for(Account acct : accounts){
    if(getMap.containsKey(acct.Industry)){
        Decimal getAnnualRevenue = getMap.get(acct.Industry);
        getMap.put(acct.Industry, getAnnualRevenue + acct.AnnualRevenue);
    }
    getMap.put(acct.Industry, acct.AnnualRevenue);
}


    return getMap;
}