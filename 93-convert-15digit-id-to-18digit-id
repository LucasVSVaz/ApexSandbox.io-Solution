public String convert15to18DigitId(String fifteenDigit)
{
    //code here
    if(fifteenDigit.length() != 15){
        return String.valueOf(-1);
    } 
    else if (String.isBlank(fifteenDigit)){
        return null;
    }

    else {
        Id result = Id.valueOf(fifteenDigit);

        return String.valueOf(result);
    }
}