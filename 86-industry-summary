public Map<String, Decimal> industrySummary(List<Account> accounts) {
    //code here

Map<String, Decimal> getMap = new Map<String, Decimal>();
    for (Account a : accounts) {
        if (getMap.containsKey(a.Industry)) {
            Decimal currentRevenue = getMap.get(a.Industry);
            getMap.put(a.Industry, currentRevenue + a.AnnualRevenue);
        }
        else {
            getMap.put(a.Industry, a.AnnualRevenue);
        }
    }
    return getMap;
}